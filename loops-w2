class Main {
  public static boolean palindrome(String s) {
      int i = 0;
      int e = s.length() - 1;

      while (i <= e) {
          if (s.charAt(i) != s.charAt(e)) {
              return false;
          }
          i++;
          e--;
      }

      return true;
  }
  public static void main(String[] args) {
    String a = "racecar";
    String b = "hello";
    System.out.println(palindrome(a));
  }
}
